{
  "name": "fastify_playground",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "build_old": "tsc -p tsconfig.json",
    "build_watch": "tsc -w -p tsconfig.json",
    "dev": "npm run build && concurrently \"tsc --watch\" \"fastify start -w -l info -T 60000 -P dist/app.js\"",
    "dev_old": "node .",
    "dev_watch": "tsx watch src/app.ts",
    "start_old": "node ./dist/app.js",
    "start": "fastify start -l info ./dist/app.js"
  },
  "dependencies": {
    "@fastify/autoload": "^6.2.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/jwt": "^9.0.4",
    "@fastify/websocket": "^11.0.2",
    "amqplib": "^0.10.5",
    "fastify": "^5.2.1",
    "fastify-cli": "^7.3.0",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/node": "^22.13.1",
    "@types/ws": "^8.5.14",
    "concurrently": "^9.1.2",
    "typescript": "^5.5.3"
  },
  "private": true
}
