{
  "name": "dashboard_service",
  "version": "1.0.0",
  "description": "servers player game history info",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist/index.js",
