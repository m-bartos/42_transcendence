    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fastify/jwt": "^9.1.0",
    "fastify": "^5.2.2",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1"
  }
}
